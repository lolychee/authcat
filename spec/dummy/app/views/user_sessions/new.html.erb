<%= form_with model: @user, url: sign_in_path(redirect_to: params[:redirect_to]), method: :post, html: { class: "pure-form pure-form-stacked sign-in-form" }, builder: Dummy::FormBuilder do |f| %>

  <%# if @user.tfa_enabled? %>
    <%#= render "tfa", f: f %>
  <%# else %>
    <%= render "password", f: f %>
  <%# end %>

<% end %>
